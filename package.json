{
  "name": "lordofapps",
  "version": "0.0.6",
  "description": "Global wrapper for app packages",
  "bin": {
    "lord": ".dist/index.js"
  },
  "scripts": {
    "check": "npm whoami && npm pack --dry-run",
    "build": "tsc --outDir ./.dist index.ts",
    "git-patch": "npm version patch",
    "git-minor": "npm version minor",
    "git-major": "npm version major",
    "push": "git push origin master --tags && npm publish",
    "patch": "run-s check build git-patch push",
    "minor": "run-s check build git-minor push",
    "major": "run-s check build git-major push"
  },
  "author": "Jaspreet Singh <dev@jas.bio>",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "typescript": "^2.9.2"
  }
}
