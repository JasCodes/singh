{
  "name": "lordofapps",
  "version": "0.0.2",
  "description": "Global wrapper for app packages",
  "bin": {
    "superpack": ".dist/index.js",
    "sp": ".dist/index.js"
  },
  "scripts": {
    "build": "tsc --outDir ./.dist index.ts",
    "npm-login-check": "npm whoami",
    "patch": "npm version patch",
    "publish": "npm publish",
    "publish-check": "npm pack --dry-run",
    "git-push": "git push origin master --tags",
    "push": "run-s build npm-login-check publish-check patch git-push"
  },
  "author": "Jaspreet Singh <dev@jas.bio>",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "typescript": "^2.9.2"
  }
}
