{
  "name": "lordofapps",
  "version": "0.0.3",
  "description": "Global wrapper for app packages",
  "bin": {
    "lord": ".dist/index.js"
  },
  "scripts": {
    "build": "tsc --outDir ./.dist index.ts",
    "npm-login-check": "npm whoami",
    "git-patch": "npm version patch",
    "git-minor": "npm version minor",
    "git-major": "npm version major",
    "publish": "npm publish",
    "publish-check": "npm pack --dry-run",
    "git-push": "git push origin master --tags",
    "check": "run-s build npm-login-check publish-check",
    "patch": "run-s git-patch git-push publish",
    "minor": "run-s git-minor git-push publish",
    "major": "run-s git-major git-push publish"
  },
  "author": "Jaspreet Singh <dev@jas.bio>",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "typescript": "^2.9.2"
  }
}
